{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "jsx": "react",
      "baseUrl": "./src",
      "paths": {
        "@components/*": ["src/components/*"],
        "@pages/*": ["src/pages/*"],
        "@assets/*": ["src/assets/*"],
        "@utils/*": ["src/utils/*"],
      }
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
  }
  